{
  "name": "my-patro-app",
  "version": "0.0.1",
  "private": true,
  "description": "A PatroCMS headless CMS application",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply DB",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:studio": "drizzle-kit studio",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "update": "pnpm add @patro-io/cms@latest",
    "update:beta": "pnpm add @patro-io/cms@beta"
  },
  "dependencies": {},
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/node": "^24.9.2",
    "drizzle-kit": "^0.30.3",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5",
    "wrangler": "^4.45.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
