{
  "name": "@patro-io/cms",
  "version": "0.1.0-beta.12",
  "description": "Core framework for Patro: An Edge-first Headless CMS built with Effect TS for Cloudflare Workers",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    },
    "./services": {
      "types": "./dist/types/services.d.ts",
      "import": "./dist/esm/services.js",
      "require": "./dist/cjs/services.cjs"
    },
    "./middleware": {
      "types": "./dist/types/middleware.d.ts",
      "import": "./dist/esm/middleware.js",
      "require": "./dist/cjs/middleware.cjs"
    },
    "./routes": {
      "types": "./dist/types/routes.d.ts",
      "import": "./dist/esm/routes.js",
      "require": "./dist/cjs/routes.cjs"
    },
    "./templates": {
      "types": "./dist/types/templates.d.ts",
      "import": "./dist/esm/templates.js",
      "require": "./dist/cjs/templates.cjs"
    },
    "./plugins": {
      "types": "./dist/types/plugins.d.ts",
      "import": "./dist/esm/plugins.js",
      "require": "./dist/cjs/plugins.cjs"
    },
    "./utils": {
      "types": "./dist/types/utils.d.ts",
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.cjs"
    },
    "./types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/esm/types.js",
      "require": "./dist/cjs/types.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "migrations",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "cms",
    "headless-cms",
    "cloudflare",
    "workers",
    "edge",
    "typescript",
    "hono",
    "content-management",
    "api",
    "patro"
  ],
  "author": "Patro <team@patro.io> (https://patro.io)",
  "license": "FSL-1.1-MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patro-io/cms.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/patro-io/cms/issues"
  },
  "homepage": "https://patro.io",
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "drizzle-orm": "^0.44.0",
    "hono": "^4.0.0"
  },
  "dependencies": {
    "effect": "^3.19.8",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251128.0",
    "@types/node": "^24.10.1",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
